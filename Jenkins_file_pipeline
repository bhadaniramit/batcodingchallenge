
pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('build') {
                    steps {
                        echo 'Building'
                        git credentialsId: 'myGitHub', url: 'https://github.com/bhadaniramit/batcodingchallenge.git'
                    }
                }
                stage('Deploy') {
                            steps {
                                echo 'Deploying'
                            }
                        }
                        stage('Test') {
                                    steps {
                                        echo 'Testing'
                                    }
                                }
                                stage('Release') {
                                            steps {
                                                echo 'Releaseing'
                                            }
                                        }
                                        stage('Generate Report') {
                                                    steps {
                                                        echo 'Generating Report'
                                                    }
                                                }
    }
}
